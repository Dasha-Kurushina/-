#include <iostream>
#include <math.h>

using namespace std;

int main (int argc, char *argv[])
{
	setlocale(0,"");
	int m, n;
	double r1, r4, x;
	bool r2, r3;
	cout << "\n m="; cin >> m;
	cout << "\n n="; cin >> n;
	cout << "\n x="; cin >> x;
	r1 = (float) m++ / n--;
	r2 = ++m < n--;
	r3 = n-- > m;
	r4 = sin(pow(x,3)) + pow(x,4) + pow(pow(x,2) + pow(x,3),1/5);
	cout << "\n r1=" << r1 << endl;
	cout << "\n r2=" << r2 << endl;
	cout << "\n r3=" << r3 << endl;
	cout << "\n r4=" << r4 << endl;

	return 0;
}

